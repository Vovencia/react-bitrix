{
  "name": "react-bitrix",
  "version": "0.0.1",
  "description": "React with bitrix",
  "homepage": "https://github.com/Vovencia/react-bitrix#readme",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "cross-env ENV=server MODE=development ts-node scripts/build.ts",
    "build:browser": "cross-env ENV=browser MODE=development ts-node scripts/build.ts",
    "build": "npm run build:server && npm run build:browser",
    "build:server:prod": "cross-env ENV=server MODE=production ts-node scripts/build.ts",
    "build:browser:prod": "cross-env ENV=browser MODE=production ts-node scripts/build.ts",
    "build:prod": "npm run build:server:prod && npm run build:browser:prod",
    "watch:server": "cross-env ENV=server MODE=development WATCH=true ts-node scripts/build.ts",
    "watch:browser": "cross-env ENV=browser MODE=development WATCH=true ts-node scripts/build.ts",
    "serve": "ts-node ./scripts/serve.ts",
    "installing": "npm i -g typescript eslint eslint-plugin-react@latest @typescript-eslint/eslint-plugin@latest @typescript-eslint/parser@latest eslint@latest"
  },
  "keywords": [
    "react",
    "bitrix",
    "ssr"
  ],
  "author": "Vovencia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vovencia/react-bitrix.git"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@types/react-dom": "^16.9.8",
    "@types/react-html-parser": "^2.0.1",
    "@types/styled-components": "^5.1.2",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "express": "^4.17.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "styled-components": "^5.1.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "react": "^16.13.1"
  }
}
